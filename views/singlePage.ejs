<!DOCTYPE html>

    <head>
        <% include head %>
        <title>Log In</title>

    </head>
    <body>
    <% include navbar %>
     <main>
		<div class="conatainer">
			<div class="row">
				<div class="col s6">
					<div class="center-align">
						<img src="<%=photo[0].$t%>" width="100%" height="300px">
					</div>
				</div>
				<div class="col s6">
					<div class="center-align">
					<% for(let i =1; i<5; i++){ %>
						<div class="col s12">
							<img src="<%=photo[i].$t%>" width ="50px" height="50px">
						</div>
					<% } %>
					</div>
				</div>
			</div>
			<div class="row">
				<div class="info center-align">
					<div class="col s2">
						Breed
					</div>
					<div class="col s2">
						Age
					</div>
					<div class="col s2">
						Gender
					</div>
					<div class="col s2">
						Owner's Name
					</div>
					<div class="col s2">
						Contact Info
					</div>
				</div>
			</div>
			<div class="row">
				<div class="info center-align">
					<div class="col s2">
						<%=breed%>
					</div>
					<div class="col s2">
						<%=age%>
					</div>
					<div class="col s2">
						<%=sex%>
					</div>
					<div class="col s2">
						Owner's Name
					</div>
					<div class="col s2">
						<%=phone%>
					</div>
				</div>
			</div>
			<div class="row">
				<div class="description">
					<%=description%>
				</div>
			</div>
		</div>
	</main>
    <% include footer %>
    </body>
</html>