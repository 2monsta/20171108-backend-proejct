<!DOCTYPE html>

    <head>
        <% include head %>
        <title>Log In</title>

    </head>
    <body>
    <% include navbar %>
     <main>
		<div class="container">
			<div class="row top-content">
				<div class="col s12">
						<h3>Meet <%=name%>:</h3>	
				</div>
			</div>
			<div class="row top-content">
				<div class="col s4">
						<img src="<%=photo%>" width="300px" height="300px" class="hoverable">
				</div>
				<div class="col s6 description flex-text">
					<h5>About</h5>
					<%=description%>
				</div>
			</div>
				<div class="row info card center-align">
					<div class="col s2">
						<h5>Breed</h5>
					</div>
					<div class="col s2">
						<h5>Age</h5>
					</div>
					<div class="col s2">
						<h5>Gender</h5>
					</div>
					<div class="col s2">
						<h5>Owner</h5>
					</div>
					<div class="col s2">
						<h5>Contact</h5>
					</div>
				</div>
			<!-- <div class="divider"></div> -->
			<div class="row card">
				<div class="info center-align">
					<div class="col s2">
						<%=breed%>
					</div>
					<div class="col s2">
						<%=age%>
					</div>
					<div class="col s2">
						<%=sex%>
					</div>
					<div class="col s2">
						<%=contactName%>
					</div>
					<div class="col s2">
						<%=contactInfo%>
					</div>
				</div>
			</div>
		</div>
	</main>
    <% include footer %>
    </body>
</html>